{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "cmd",
          "cmd": "cmd",
          "args": true
        },
        {
          "name": "sh",
          "cmd": "sh",
          "args": true
        }
      ]
    },
    {
      "identifier": "shell:allow-stdin-write",
      "allow": [
        {
          "name": "cmd",
          "cmd": "cmd",
          "args": true
        },
        {
          "name": "sh",
          "cmd": "sh",
          "args": true
        }
      ]
    },
    {
      "identifier": "shell:allow-kill",
      "allow": [
        {
          "name": "cmd",
          "cmd": "cmd",
          "args": true
        },
        {
          "name": "sh",
          "cmd": "sh",
          "args": true
        }
      ]
    },
    "shell:allow-open",
    "os:default",
    "clipboard-manager:allow-read-text"
  ]
}